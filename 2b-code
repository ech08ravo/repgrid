<script nonce="b1c1cc3a9dd486ed554e81bc830c0114">
    $(document).ready(function(){
        $("#column_jumper").chosen({
            width: '100%',
            search_contains: true,
            placeholder_text_single: 'Jump to a column'
        })
    });
    $(document).on('change', '#column_jumper', function(){
        if ($('#column_jumper').val()) {
            window.location = '/tables/E7A96D45_8180_4181_9A7270F845FF8925/columns/__column_uuid__/edit'.replace('__column_uuid__', $('#column_jumper').val());
        }
    });
</script>


