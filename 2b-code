        var sresXMLOpen = XMLHttpRequest.prototype.open;
        XMLHttpRequest.prototype.open = function(){
            var res = sresXMLOpen.apply(this, arguments);
            var err = new Error();
            this.setRequestHeader('X-CSRFToken', ENV['CSRF_TOKEN']);
            return res;
        }
